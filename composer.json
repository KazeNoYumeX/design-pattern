{
    "name": "kevinsmith/no-framework",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "laminas/laminas-diactoros": "^3.2",
        "middlewares/fast-route": "^2.0",
        "middlewares/request-handler": "^2.0",
        "narrowspark/http-emitter": "^2.0",
        "php-di/php-di": "^7.0",
        "relay/relay": "^2.1"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "pestphp/pest": "^2.23"
    },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
