{
    "name": "kazenoyumex/card-game-framework",
    "type": "project",
    "require": {
        "php": "^8.1|^8.2",
        "fakerphp/faker": "^1.23"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "pestphp/pest": "^2.32"
    },
  "autoload": {
    "psr-4": {
      "CardGameFramework\\": "src/"
    }
  },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
