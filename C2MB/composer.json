{
    "name": "kazenoyumex/big-two",
    "type": "project",
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "laravel/pint": "^1.21",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin-faker": "^3.0",
        "symfony/var-dumper": "^7.2"
    },
  "autoload": {
    "psr-4": {
      "C2MB\\": "src/"
    }
  },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
